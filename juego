package ProyectoFinal;

class juego {

	private int[] premios;
    private String comodines;
    private int numero;
    private int numero2;
    private int numero3;

    // Constructor
    public juego(String comodines, int[] premios, int numero, int numero2, int numero3) {
        this.comodines = comodines;
        this.premios = premios;
        this.numero = numero;
        this.numero2 = numero2;
        this.numero3 = numero3;
    }

    // MÃ©todos getter y setter para encapsulamiento
    public String getComodines() {
        return comodines;
    }

    public void setComodines(String comodines) {
        this.comodines = comodines;
    }
    
    public int[] getPremios() {
        return premios;
    }

    public void setPremios(int[] premios) {
        this.premios = premios;
    }
    
    public int getNumero() {
        return numero;
    }

    public void setNumero(int numero) {
        this.numero = numero;
    }
    
    public int getNumero2() {
        return numero2;
    }

    public void setNumero2(int numero2) {
        this.numero2 = numero2;
    }
    
    public int getNumero3() {
        return numero3;
    }

    public void setNumero3(int numero3) {
        this.numero3 = numero3;
    }
}
